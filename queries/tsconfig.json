{
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types"],
    "types": ["node"],
    "module": "commonjs",
    "moduleResolution": "node",
    "pretty": true,
    "sourceMap": true,
    "target": "es6",
    "lib": ["dom", "esnext"],
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "alwaysStrict": false,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "*": ["node_modules/*"],
      "@aut-labs/sdk": ["./sdk/src/index.ts"],
      "@aut-labs/sdk/dist/*": ["./sdk/src/*"],
      "@aut-labs/sdk/*": ["./sdk/src/*"],
      "@sdk-models/*": ["./sdk/src/models/*"],
      "@sdk-services/*": ["./sdk/src/services/*"],
      "@sdk-utils/*": ["./sdk/src/utils/*"],
      "@sdk-contracts/*": ["./sdk/src/contracts/*"],
      "@aut-labs/abi-types": ["./sdk/src/abi-types/index.ts"],
      "@aut-labs/abi-types/*": ["./sdk/src/abi-types/*"]
    }
  },
  "include": ["src/**/*.ts", "src/signature.js"],
  "exclude": ["node_modules"]
}
